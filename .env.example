# Local Development Environment Variables
# Copy this file to .env and fill in your values

# YouTube Proxy Configuration
# Use local proxy for development
YOUTUBE_PROXY_URL=http://127.0.0.1:3000/proxy?url=

# Or use Render proxy for testing production
# YOUTUBE_PROXY_URL=https://youtube-proxy.onrender.com/proxy?url=

# Node Environment
NODE_ENV=development

# Port for local proxy server
PORT=3000

# API Configuration (if needed)
# API_KEY=your-api-key-here
# API_SECRET=your-api-secret-here

# Logging (optional)
# LOG_LEVEL=debug
# DEBUG=true

# Rate Limiting (optional)
# RATE_LIMIT_REQUESTS=100
# RATE_LIMIT_WINDOW=60000

# CORS Configuration
# ALLOWED_ORIGINS=http://localhost:3000,https://yourdomain.com
# ALLOW_CREDENTIALS=true
